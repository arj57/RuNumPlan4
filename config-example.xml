<data>
    <DryRun type="bool">false</DryRun>
    <InputDataDir>InputData</InputDataDir>
    <Src>
        <Urls>
<!--            <Url>file:testData/ABC-3xx.csv</Url>-->
<!--            <Url>file:testData/ABC-4xx.csv</Url>-->
<!--            <Url>file:testData/ABC-8xx.csv</Url>-->
            <Url>file:testData/DEF-9xx.csv</Url>
<!--             <Url>https://opendata.digital.gov.ru/downloads/ABC-3xx.csv</Url>-->
<!--             <Url>https://opendata.digital.gov.ru/downloads/ABC-4xx.csv</Url>-->
<!--             <Url>https://opendata.digital.gov.ru/downloads/ABC-8xx.csv</Url>-->
<!--             <Url>https://opendata.digital.gov.ru/downloads/DEF-9xx.csv</Url>-->
        </Urls>
        <CsvExtraParameters>
            <delimiter>;</delimiter>
            <lineterminator>\r\n</lineterminator>
        </CsvExtraParameters>
    </Src>
    <Dst>
        <Url>mysql://login:passwd@host_ip/database</Url>
    </Dst>

    <Converter>
        <OnKeyErrorAction> WARNING</OnKeyErrorAction>
        <SrcFields>
            <Field>
                <Name>Abc</Name>
            </Field>
            <Field>
                <Name>From</Name>
            </Field>
            <Field>
                <Name>To</Name>
            </Field>
            <Field>
                <Name>Capacity</Name>
            </Field>
            <Field>
                <Name>Operator</Name>
            </Field>
            <Field>
                <Name>Region</Name>
            </Field>
            <Field>
                <Name>GarRegion</Name>
            </Field>
            <Field>
                <Name>Inn</Name>
            </Field>
        </SrcFields>
        <DstFields>
            <Field>
                <Name>code</Name>
                <Source>Abc</Source>
                <Type>int</Type>
            </Field>
            <Field>
                <Name>start</Name>
                <Source>From</Source>
                <Type>int</Type>
            </Field>
            <Field>
                <Name>end</Name>
                <Source>To</Source>
                <Type>int</Type>
            </Field>
            <Field>
                <Name>op_inn</Name>
                <Source>Inn</Source>
<!--                <Type>int</Type>-->
            </Field>
            <Field>
                <Name>loc_id</Name>
                <Source>GarRegion</Source>
                <Type>str</Type>
<!--                <Action>AddToLocationMap</Action>-->
            </Field>
        </DstFields>

    </Converter>
    <Logging>
        <LogFile>numplan.log</LogFile>
        <!--FATAL, ERROR, WARNING, INFO, DEBUG-->
        <LogLevel>DEBUG</LogLevel>
        <Silence type="bool">false</Silence>
    </Logging>

</data>